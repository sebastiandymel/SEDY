<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:system="clr-namespace:System;assembly=mscorlib"
                    >
    <Color x:Key="Color.Primary">#e2e2e2</Color>
    <Color x:Key="Color.Secondary">#1c1c1c</Color>
    <Color x:Key="Color.Background.Expander.MouseOver">#f0f0f0</Color>

    <SolidColorBrush x:Key="Brush.TextBlock.Header" Color="Black" />
    <SolidColorBrush x:Key="Brush.TextBox.Selection" Color="#4F5558" />

    <BitmapImage x:Key="Image.Logo" UriSource="pack://application:,,,/Remedy.CommonUI;component/Skins/50shadesgrey.png" />

    <system:String x:Key="String.WindowTitle">50 Shades of Gray</system:String>

</ResourceDictionary>